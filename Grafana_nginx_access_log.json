          "rawSql": "SELECT\r\n  time,\r\n  if(isNaN(json_rpc), 0, json_rpc) AS \"JSON-RPC API\",\r\n  if(isNaN(js_rpc), 0, js_rpc) AS \"JS-RPC\",\r\n  if(isNaN(charts), 0, charts) AS \"Charts\",\r\n  if(isNaN(zabbix_ui), 0, zabbix_ui) AS \"Zabbix UI\",\r\n  if(isNaN(history), 0, history) AS \"History\",\r\n  if(isNaN(other), 0, other) AS \"Other\"\r\nFROM (\r\n  SELECT\r\n    toStartOfMinute(timestamp) AS time,\r\n    maxIf(body_bytes_sent, request LIKE '%api_jsonrpc.php%') AS json_rpc,\r\n    maxIf(body_bytes_sent, request LIKE '%jsrpc.php%') AS js_rpc,\r\n    maxIf(body_bytes_sent, request LIKE '%chart%') AS charts,\r\n    maxIf(body_bytes_sent, request LIKE '%zabbix.php%') AS zabbix_ui,\r\n    maxIf(body_bytes_sent, request LIKE '%history%') AS history,\r\n    maxIf(body_bytes_sent, NOT (\r\n      request LIKE '%api_jsonrpc.php%' OR\r\n      request LIKE '%jsrpc.php%' OR\r\n      request LIKE '%chart%' OR\r\n      request LIKE '%zabbix.php%' OR\r\n      request LIKE '%history%'\r\n    )) AS other\r\n  FROM zabbix_logs.nginx_access_extended\r\n  WHERE\r\n    $__timeFilter(timestamp)\r\n    AND body_bytes_sent IS NOT NULL\r\n  GROUP BY time\r\n)\r\nORDER BY time;",
